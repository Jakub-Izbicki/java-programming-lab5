<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%valuesText"
          GridPane.halignment="CENTER" GridPane.columnSpan="4">
        <font>
            <Font size="20.0"/>
        </font>
    </Text>

    <HBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <GridPane fx:id="valuesAGridPane" alignment="CENTER" hgap="10.0" prefHeight="287.0" prefWidth="297.0"
                      vgap="10.0">
                <children>

                </children>
                <padding>
                    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0"/>
                </padding>
            </GridPane>


            <GridPane fx:id="valuesBGridPane" alignment="CENTER" hgap="10.0" prefHeight="287.0" prefWidth="297.0"
                      vgap="10.0">
                <children>

                </children>
                <padding>
                    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0"/>
                </padding>
            </GridPane>

        </children>
    </HBox>

    <Button fx:id="matrixValuesReadyButton" onAction="#onMatrixValuesReadyButton"
            mnemonicParsing="false" text="%matrixValuesReady" GridPane.columnIndex="1"
            GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.rowIndex="4"/>

    <HBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <Text fx:id="errorInfo" styleClass="errorText" strokeType="OUTSIDE" strokeWidth="0.0"
                  GridPane.columnSpan="4"
                  GridPane.halignment="CENTER" GridPane.rowIndex="6"/>
        </children>
        <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="10.0"/>
        </padding>
    </HBox>
</VBox>